{
    "env": {
        "es2021": true,
        "node": true,
        "browser": true
    },
    "extends": "airbnb-base",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "no-unused-vars": "warn",
        "import/prefer-default-export": "off",
        "no-restricted-syntax": "off"
    }
}